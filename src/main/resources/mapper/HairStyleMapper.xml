<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.HairStyleMapper">
	<select id="getHairStyleList" parameterType="com.example.demo.model.HairStyle">
		select * from hairStyle
	</select>

	<select id="getHairStyle" parameterType="Long" resultType="com.example.demo.model.HairStyle">
		select * from hairStyle where styleNo = #{styleNo}
	</select>
	<select id="insertHairStyle" parameterType="Long" resultType="com.example.demo.model.HairStyle">
		select * from hairStyle where styleNo = #{styleNo}
	</select>
	<select id="updateHairStyle" parameterType="Long" resultType="com.example.demo.model.HairStyle">
		select * from hairStyle where styleNo = #{styleNo}
	</select>
	<select id="deleteHairStyle" parameterType="Long" resultType="com.example.demo.model.HairStyle">
		delete hairStyle where styleNo = #{styleNo}
	</select>


</mapper>