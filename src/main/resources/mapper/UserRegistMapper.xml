<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.UserRegistMapper">
	<select id="getUser" parameterType="String" resultType="com.example.demo.model.UserRegist">
		SELECT * FROM userRegist WHERE userId = #{userId}
	</select>
	<select id="getDisigner" parameterType="String" resultType="com.example.demo.model.UserRegist">
		SELECT * FROM userRegist WHERE userName = #{userName}
	</select>
</mapper>